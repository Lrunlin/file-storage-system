{
  "name": "file-storage-system",
  "version": "1.0.0",
  "author": "L",
  "types": "index.d.ts",
  "scripts": {
    "dev": "tsx watch --include \"./src/**/*\" --clear-screen=false ./index.ts",
    "push": "npm publish",
    "test": "tsx watch --include \"./test.ts\" --clear-screen=false ./test.ts",
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json&&javascript-obfuscator ./dist --output ./dist",
    "t": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json"
  },
  "exports": {
    ".": {
      "node": {
        "require": "./dist/cjs/index.js",
        "import": "./dist/esm/index.js"
      },
      "default": "./dist/esm/index.js"
    }
  },
  "description": "",
  "keywords": [
    "file-storage-system",
    "oss",
    "tool"
  ],
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "dependencies": {
    "axios": "^1.7.9",
    "js-base64": "^3.7.7",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "sha1": "^1.1.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/md5": "^2.3.5",
    "@types/node": "^20.12.11",
    "@types/sha1": "^1.1.5",
    "@types/uuid": "^10.0.0",
    "typescript": "^5.4.5"
  }
}
